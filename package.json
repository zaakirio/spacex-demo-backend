{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend",
  "main": "server.ts",
  "scripts": {
    "start": "serverless offline start --stage local --noPrependStageInUrl",
    "spec": "node -r dotenv/config node_modules/.bin/jest --runInBand --testTimeout=30000",
    "populate-app": "node_modules/.bin/ts-node helpers/populate.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "ci": "yarn lint && yarn spec",
    "generate:codegen": "graphql-codegen --config codegen.yml",
    "deploy": "chmod +x server.ts && ./node_modules/.bin/serverless deploy"
  },
  "author": "app",
  "license": "ISC",
  "jest": {
    "globals": {
      "transform": {
        "^.+\\.tsx?$": [
          "ts-jest",
          {
            "diagnostics": false
          }
        ]
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true
  },
  "dependencies": {
    "@apollo/server": "^4.2.2",
    "@faker-js/faker": "^7.6.0",
    "@sentry/serverless": "^7.23.0",
    "@sentry/tracing": "^7.23.0",
    "@supercharge/promise-pool": "^2.3.2",
    "@vendia/serverless-express": "^4.10.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.0",
    "luxon": "^3.1.1",
    "mysql2": "^2.3.3",
    "rate-limiter-flexible": "^2.4.1",
    "request-ip": "^3.3.0",
    "sequelize": "^6.26.0",
    "umzug": "^3.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.15.0",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.8.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.8",
    "@types/bluebird": "^3.5.38",
    "@types/express-jwt": "^7.4.2",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/luxon": "^3.1.0",
    "@types/node": "^18.11.10",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@types/umzug": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "apollo": "^2.34.0",
    "aws-sdk": "^2.1267.0",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "serverless": "^3.25.1",
    "serverless-offline": "^11.6.0",
    "serverless-plugin-typescript": "^2.1.4",
    "supertest": "^6.3.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
